

@{
    ViewBag.Title = "All Users";
}



@model Task<List<USER_API.Authontaction.ApplicationUser>>

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">@ViewBag.Title</h4>
            <div class="table-responsive">
                <table class="table table-dark">
                    <thead>
                        <tr>
                            <th> # </th>
                            <th> UserName </th>
                            <th> User Email </th>
                        </tr>
                    </thead>
                    <tbody>
                        @{ var i = 1;}
                        @foreach (var item in await Model)
                        {

                        <tr>
                            <td>@i</td>
                            <td>@item.UserName</td>
                            <td>@item.Email</td>
                        </tr>
                            i++;
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


